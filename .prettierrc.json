{
  "printWidth": 80,
  "tabWidth": 4,
  "singleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "parser": "babel",
  "semi": true,
  "requirePragma": false,
  "proseWrap": "always",
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "css",
  "quoteProps": "as-needed",
  "vueIndentScriptAndStyle": false
}
